{
  "name": "back-end-dev-nestify",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "dev": "ts-node-dev --respawn --require tsconfig-paths/register src/index.ts",
    "migrate": "ts-node --require tsconfig-paths/register -- src/db/run_migration.ts",
    "seed": "ts-node --require tsconfig-paths/register -- src/db/run_seeder.ts"
  },
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.4.2",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
